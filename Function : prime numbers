#include<iostream>
using namespace std;

bool prime(int n){
    int c=0;
    for(int i=2;i<n;i++) {
        if (n % i == 0) {
            return false;
        }
        c=1;
    }
    if(c==1){
        return true;
        }
}

int main() {
    // Write your code here
    int n, r;
    cin>> n;
    bool ans= prime(n);
    cout<< ans;

}
