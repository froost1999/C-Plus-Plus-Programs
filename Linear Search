#include <iostream>
using namespace std;


int main() {
    int arrays[100];
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>arrays[i];
    }
    int search;
    cout<<"Enter what to search";
    cin>>search;
    for(int i=0;i<n;i++){
        if(search==arrays[i]){
            cout<<"Element found at"<<i<<"pos";
        }
    }
}
